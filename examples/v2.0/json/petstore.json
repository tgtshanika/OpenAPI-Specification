swagger: "2.0"
info:
  description: "This API provides a procedural based REST compliant Micro Service Interface for the Enrolment Services micro service.  For this API, you can use the api key special-key to test the authorization filters."
  version: "1.0"
  title: Enrolment_Services_Micro_Service
  license:
    name: Copyright (c) 2014-2015 Discovery Partner Markets. All Rights Reserved.
basePath: /vitality-enrolment-services-service
paths:
  "/svc/{tenant}/activateMembership":
    post:
      tags:
        - Activate Membership
      operationId: POST_ACTIVATE_MEMBERSHIP
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - in: body
          name: activateMembershipInboundPayload
          required: true
          schema:
            $ref: "#/definitions/activateMembershipInboundPayload"
        - name: tenant
          in: path
          description: The tenant associated to the market.
          required: true
          type: integer
          format: int32
        - name: correlation-Id
          in: header
          description: A partner created identifier to identify each message.
          required: false
          type: string
        - name: locale
          in: header
          description: Set to the Locale of the member and will be used for language determination. This must be a valid ISO639 language code.
          required: false
          type: string
        - name: message-Id
          in: header
          description: A Vitality Group crated unique identifier to identify each message.
          required: false
          type: string
        - name: session-Id
          in: header
          description: Identifier used to group messages together. Where the same session-Id can be used for multiple calls that belong to the same session.
          required: false
          type: string
      responses:
        "200":
          description: Success
          schema:
            $ref: "#/definitions/activateMembershipOutBoundPayload"
        "400":
          description: Bad Request
          schema:
            $ref: "#/definitions/Errors"
        "500":
          description: Internal Server Error
          schema:
            $ref: "#/definitions/Errors"
      x-auth-type: "Application & Application User"
      x-throttling-tier: Unlimited
